{
  "manifest_version": 2,
  "name": "Track Stack Overflow",
  "version": "0.1",
  "permissions": ["tabs","http://*/*","https://*/*"],
  "content_scripts": [
  {
    "matches": ["*://*.stackoverflow.com/*"],
    "js": ["contentscript.js"]
  }
  ],
  "page_action": {
  	"default_icon": "icon.png"
  }
}

